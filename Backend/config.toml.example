# 复制为 config.toml 并按需填写（若采用 toml 配置，需在应用内增加对 toml 的读取逻辑）
# 当前版本以环境变量 / .env 为主，本文件仅作说明与示例

[llm]
[llm.moonshot]
api_key = ""
base_url = "https://api.moonshot.cn/v1"
model = "moonshot-v1-32k"

[llm.glm]
api_key = ""
base_url = "https://open.bigmodel.cn/api/paas/v4/"
model = "GLM-4"

[llm.openai]
api_key = ""
base_url = "https://api.openai.com/v1"
model = "gpt-4o-mini"

[llm.coze]
api_key = ""
base_url = "https://api.coze.cn/open_api/v2/chat"
model = ""

